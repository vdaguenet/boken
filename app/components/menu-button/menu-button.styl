.menu-button
  .picto
    width 100px
    height 100px
    border-radius 50px
    background #f5f1e6
    margin 30px auto 11px auto

    img
      width 68%
      position relative
      top 50%
      left 50%
      transform translateX(-50%) translateY(-50%)

  p
    text-align center
    color #7b705a
    text-transform uppercase
    font-size 15px