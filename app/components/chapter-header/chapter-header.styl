.chapter-header
  color brown
  text-align center
  text-transform uppercase
  margin-top 75px

  .header
    margin-bottom 20px

    &:before,
    &:after
      content ''
      width 113px
      height 1px
      background brown
      display inline-block
      position relative
      top -5px

    &:before
      left -15px

    &:after
      left 15px

  h1
    font-size 25px